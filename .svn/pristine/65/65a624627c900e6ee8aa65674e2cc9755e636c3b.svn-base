/* pages/order/commentList/commentList.wxss */
.tabs {
  height: 90rpx;
  /*tab页面*/
}
.tabs .t_title {
  height: 90rpx;
  display: flex;
  font-size: 30rpx;
  align-items: center;
}
.tabs .t_title .item {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.tabs .t_title .item .t_font {
  height: 86rpx;
  line-height: 86rpx;
  color: #303030;
}
.tabs .t_title .item .t_line {
  height: 4rpx;
  background-color: rgba(0, 0, 0, 0);
  width: 60rpx;
}
.tabs .t_title .active .t_font {
  height: 86rpx;
  line-height: 86rpx;
  color: #eb5902;
}
.tabs .t_title .active .t_line {
  height: 4rpx;
  background-color: #eb5902;
  width: 60rpx;
}
.tabs .bg_img {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 800rpx;
}
.tabs .bg_img img {
  width: 242rpx;
  height: 244rpx;
  margin-bottom: 40rpx;
}
.tabs .bg_img p {
  height: 80rpx;
  font-size: 28rpx;
  color: #999;
}
.tabs .loadp {
  font-family: '微软雅黑';
  font-size: 28rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
}
/*未使用*/
.not_user {
  padding: 44rpx;
}
.not_user .li {
  height: 258rpx;
  border: 1rpx solid #f3604e;
  border-radius: 10rpx;
  background-color: #f1D6D7;
  overflow: hidden;
  margin: 0 auto;
  margin-bottom: 24rpx;
}
.not_user .li .s_top {
  height: 64rpx;
  padding: 20rpx 28rpx 16rpx;
  background-color: #f3604e;
  display: flex;
  justify-content: space-around;
}
.not_user .li .s_top .t_left {
  display: flex;
  flex-direction: column;
  width: 260rpx;
}
.not_user .li .s_top .t_left .span {
  height: 28rpx;
  font-size: 28rpx;
  color: #fff;
  margin-bottom: 14rpx;
}
.not_user .li .s_top .t_left .i {
  height: 20rpx;
  font-size: 20rpx;
  color: #fff;
}
.not_user .li .s_top .t_right {
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  flex: 1;
}
.not_user .li .s_top .t_right .span {
  display: inline-block;
  width: 26rpx;
  height: 32rpx;
  cursor: pointer;
}
.not_user .li .s_top .t_right .span image {
  width: 26rpx;
  height: 32rpx;
  cursor: pointer;
}
.not_user .li .s_top .t_right .i {
  font-size: 22rpx;
  color: #fff;
  margin-top: 15rpx;
}
.not_user .li .s_bot {
  display: flex;
  height: 160rpx;
  justify-content: center;
  align-items: center;
}
.not_user .li .s_bot .b_left .left {
  height: 72rpx;
  width: 260rpx;
  display: flex;
  justify-content: center;
  align-items: baseline;
  border-right: 1rpx solid #f3604e;
}
.not_user .li .s_bot .b_left .left .i {
  font-size: 26rpx;
  color: #f3604d;
}
.not_user .li .s_bot .b_left .left .span {
  height: 66rpx;
  font-size: 66rpx;
  color: #f36150;
  margin-left: 10rpx;
}
.not_user .li .s_bot .b_left .left .em {
  font-size: 36rpx;
  color: #f36150;
}
.not_user .li .s_bot .b_right {
  flex: 1;
  padding: 60rpx;
  font-size: 30rpx;
  color: #f36150;
  display: flex;
  justify-content: center;
  align-items: center;
}
.not_user .nodata {
  height: 750rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.not_user .nodata image {
  width: 152rpx;
  height: 182rpx;
}
.not_user .nodata text {
  color: #999;
  font-size: 30rpx;
  margin-top: 16rpx;
}
.not_user .hid {
  display: none;
}
