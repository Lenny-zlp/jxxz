/*弹层*/
.elastic {
  cursor: pointer;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  position: fixed;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: rgba(0, 0, 0, 0.3);
  z-index: 2;
}
.elastic .menu {
  cursor: pointer;
  width: 600rpx;
  max-height: 632rpx;
  padding: 40rpx;
  border-radius: 10rpx;
  background-color: #fff;
  z-index: 3;
}
.elastic .menu .s_font {
  font-size: 32rpx;
  color: #303030;
  height: 82rpx;
  /*line-height:62rpx;*/
  text-align: center;
}
.elastic .menu p {
  font-size: 28rpx;
  height: 40rpx;
  color: #303030;
  line-height: 40rpx;
}
.elastic .menu .point {
  height: 450rpx;
  line-height: 450rpx;
  text-align: center;
  font-size: 36rpx;
  color: #999;
}
.elastic .menu ul::-webkit-scrollbar {
  display: none;
}
.elastic .menu ul {
  max-height: 450rpx;
  overflow-y: scroll;
}
.elastic .menu ul li {
  height: 100rpx;
  padding: 20rpx 0;
  border-bottom: 1rpx solid #ccc;
}
.elastic .menu ul li .top {
  height: 46rpx;
  line-height: 46rpx;
  display: flex;
  justify-content: space-between;
}
.elastic .menu ul li .top span {
  font-size: 26rpx;
}
.elastic .menu ul li .top i {
  font-size: 22rpx;
  color: #f36150;
}
.elastic .menu ul li .top .red {
  color: #f36150;
}
.elastic .menu ul li .top .green {
  color: rgba(13, 152, 13, 0.84);
}
.elastic .menu ul li .bot {
  height: 46rpx;
  line-height: 46rpx;
  display: flex;
  justify-content: space-between;
}
.elastic .menu ul li .bot span {
  font-size: 26rpx;
  color: #999;
}
.elastic .menu ul li .bot i {
  font-size: 22rpx;
  color: #999;
}
.elastic .menu ul li:last-child {
  border-bottom: 0;
}
.elastic .menu .inown {
  height: 50rpx;
  text-align: center;
  margin-top: 6rpx;
}
.elastic .menu .inown span {
  cursor: pointer;
  display: inline-block;
  width: 206rpx;
  height: 48rpx;
  line-height: 48rpx;
  text-align: center;
  font-size: 24rpx;
  color: #eb5902;
  border: 1rpx solid #eb5902;
  border-radius: 25rpx;
}
.m_tit {
  height: 50rpx;
  line-height: 50rpx;
  margin-left: 34rpx;
  font-size: 28rpx;
  color: #303030;
}
.menu {
  max-height: 480rpx;
  padding: 20rpx 34rpx;
  overflow-y: scroll;
}
.menu .n_item {
  height: 96rpx;
  line-height: 96rpx;
  text-align: center;
  color: #999;
}
.menu .m_item {
  height: 96rpx;
  padding: 14rpx 0;
  border-bottom: 1rpx solid #ccc;
}
.menu .m_item .m_top {
  height: 46rpx;
  line-height: 46rpx;
  display: flex;
  justify-content: space-between;
}
.menu .m_item .m_top .t_left {
  font-size: 26rpx;
}
.menu .m_item .m_top .t_right {
