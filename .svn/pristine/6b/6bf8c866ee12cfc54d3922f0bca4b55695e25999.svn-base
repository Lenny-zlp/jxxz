page{background:#eee;}
.logistice-wrap{background:#fff;padding:26rpx 28rpx;width:100%;box-sizing: border-box;margin-bottom: 10rpx;position:relative;}
.logistice-wrap image{width:120rpx;height:120rpx;margin-right:30rpx;box-sizing: border-box;border:1px solid #f8f8f8;}
.logistice-wrap text{color:#999;font-size:24rpx;}
.list-wrap{width:100%;background:#fff;overflow: hidden;margin-bottom:20rpx;}
.newMessge{width:100%;padding:0 28rpx;box-sizing: border-box;}
.newMessge>view{padding:26rpx 0;border-bottom:1px solid #eee;}
.newMessge>view text:first-child{padding-bottom:14rpx;line-height:40rpx; color:#666;}
.newMessge>view:first-child text{color:#eb5902 !important}
.morearrow{position:absolute;width:44rpx;height:44rpx;text-align: right;right:20rpx;top:50%;margin-top:-22rpx;background:url(https://cache.jiangxinxiaozhen.com/WD/APP/little/arrowB.png) no-repeat right center;background-size:25rpx 13rpx;}
.morearrow.cur{background:url(https://cache.jiangxinxiaozhen.com/WD/APP/little/arrowT.png) no-repeat right center;background-size:25rpx 13rpx;}
.copynum{padding:3rpx 12rpx;border:1rpx solid #f4f4f4;border-radius: 6rpx;margin-left:20rpx;color:#666;display:inline;}
.storeText{position:absolute;right:60rpx;top:26rpx;color:#303030 !important;font-size: 30rpx;}