page{background:#eee;}
from{width:100%;display:block;background:#fff;overflow: hidden;}
.headImg{background:#fff;width:100%;overflow: hidden;display:flex;flex-direction: column;justify-content: center;border-bottom:8rpx solid #eee;}
.headImg image{width:100rpx;height:100rpx;border-radius: 100%;margin:60rpx auto 30rpx;border:1px solid #eee;}
.headImg text{padding-bottom:40rpx;width:100%;display:block;color:#999;text-align: center;}
.personalList-wrap{width:100%;border-bottom:8rpx solid #eee;padding:0 25rpx;box-sizing: border-box;background:#fff;}
.personalList{width:100%;padding:30rpx 0;border-bottom:1px solid #eee;align-items: center;}
button{width:60%;height:80rpx;line-height: 80rpx;border:1px solid #dedddd;border-radius: 10rpx !important; background:#fff;margin:70rpx auto;}
input{font-size:26rpx !important;text-align: right;}
.checkdeWrap{width:100%;height:100%;background:rgba(0,0,0,.4);position:fixed;top:0;left:0;z-index: 10}
.cameraBtn{position:absolute;bottom: 0;left:0;padding:0 30rpx;box-sizing: border-box;width:100%;}
.cameraBtn button{width:100%;margin:15rpx 0;}
.sexWrap{width:100%;padding:0 25rpx;box-sizing: border-box;background:#fff;border-radius: 15rpx;}
.sexWrap text{display:block;width:100%;text-align: center;line-height:45rpx;padding:15rpx 0;}
.sexWrap text:first-child{border-bottom:1px solid #eee;}