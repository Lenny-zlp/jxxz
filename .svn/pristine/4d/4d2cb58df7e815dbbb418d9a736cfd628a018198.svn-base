page{background:#f8f8f8;}
.form-wrap{background:#fff;}
.formList{padding:10rpx 25rpx;background:#fff;line-height:90rpx;border-bottom:1rpx solid #eee;}
.formList button{color:#666;border:1px solid #999;border-radius: 3px;line-height:50rpx;padding:5rpx 12rpx;}
.submitBtn{width:100%;height:80rpx;background:#eb5902;color:#fff;text-align: center;line-height:80rpx;position:fixed;bottom:0;left:0;border-radius: 0 !important;}
.formList label{color:#666;font-size:28rpx;}
