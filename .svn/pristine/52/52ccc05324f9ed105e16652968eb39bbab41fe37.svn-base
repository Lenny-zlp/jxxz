page{background:#fff;}
button::after {border: none;}
button{background:none;border-radius: 0 !important;font-size:28rpx;margin: 0;}
input{ outline: none;border: none;list-style: none;font-size:28rpx;}
view,input,button,textarea{font-size:28rpx;}
.input-placeholder{color:#999;}
text{font-size:26rpx;}
text,view,button,input,textarea{font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;}
.row,.row-center,.row-between{display:flex;flex-direction: row;}
.column,.column-center,.column-between{display:flex;flex-direction: column;}
.row-center,.column-center{align-items: center;}
.row-between,.column-between{justify-content: space-between;}
/* ::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
} */
rich-text .rich-img{display:block;width:100%;margin:10rpx 0 20rpx;}
.arrowRight{background:#fff url(https://cache.jiangxinxiaozhen.com/Content/wd/more1.png)no-repeat right 25rpx center;background-size:14rpx 24rpx;}
.wx-switch-input{width:34px !important;height:18px !important;background:#eb5902 !important;border:0 none !important;}
.wx-switch-input::before{width:36px !important;height: 20px !important;background:#f8f8f8 !important}
.wx-switch-input::after{width: 18px !important;height: 18px !important;}
switch{color:#eb5902 !important;margin-left:10rpx;}
.leftArrow{background:#fff url(https://cache.jiangxinxiaozhen.com/Content/wd/more1.png)no-repeat right center;background-size:14rpx 24rpx;}
/* 自定义顶部导航样式 */
.headBg{width:100%;height:44px;text-align: left;line-height:44px;color:#fff;background:#eb5902;}
.backBtn{font-size:22px;color:#fff;font-weight: 500;height:44px; width:60rpx;padding-left:20rpx;display:inline-block;position: relative;z-index: 2;background:url(https://cache.jiangxinxiaozhen.com/WD/APP/little/whiteBack.png) no-repeat left 20rpx center;background-size:20rpx 36rpx;}
.headTitle{position:absolute;left:0;width:100%;height:44px;text-align: center;}
.weui-navbar__title {
  font-size: 14px;
}

