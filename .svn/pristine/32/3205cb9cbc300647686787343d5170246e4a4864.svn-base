page{background:#eee;}
.returnAddress{width:100%;background:#fff;padding:30rpx;box-sizing: border-box;margin-top:10rpx;}
.returnAddress view{color:#303030;font-size:32rpx;}
.returnAddress text{color:#999;padding:15rpx 0 5rpx;}
form{display:block;width:100%;overflow: hidden;background:#fff;margin-top:10rpx;}
form view{align-items: center;height:80rpx;width:100%;padding:0 30rpx;border-bottom: 1px solid #eee;}
form button{position:fixed;bottom:0;left:0;width:100%;height:88rpx;line-height: 88rpx;background:#eb5902;color:#fff;}
.returnText{margin:10rpx 30rpx 88rpx;text-align: justify;}