.content{width:600rpx;overflow: hidden;margin: 110rpx auto 0;display:block;}
.telInp{width:100%;border-bottom:1rpx solid #f4f4f4;padding-bottom:24rpx;font-size:30rpx;margin-bottom:66rpx;}
.telInp.row-between{position: relative;}
.telInp.row-between input{width:100%;}
.getWord{position:absolute;right:0;bottom:18rpx;min-width: 168rpx;height:54rpx;border-radius: 6rpx !important;line-height:52rpx;border:1rpx solid #eb5902;box-sizing: border-box;color:#eb5902;padding:0 25rpx;background:#fff;font-size:26rpx;z-index: 5;}
.sureBtn{width:100%;font-size:32rpx;height:90rpx;background:#eb5902;color:#fff;border-radius: 6rpx !important;line-height:90rpx;margin-top:112rpx;}
.getWord[disabled]{background:#fff !important;color:#999 !important;min-width: 128rpx;border:1rpx solid #999;}