.purchase-tem-wrap{width:100%;height:300rpx;padding-bottom: 25rpx;position:relative;}
.purchase-tem-wrap.over-tem::after,.moduleRem-tem-wrap.over-tem::after{position:absolute;width:140rpx;height:140rpx;content:'已抢完';line-height:140rpx;text-align: center;color:#fff;font-weight: 500;background:rgba(0,0,0,.2);border-radius: 50%;left:80rpx;top:50%;margin-top:-70rpx;}
.moduleRem-tem-wrap.over-tem::after{content: '已售罄'}
.purchase-tem-wrap image{width:300rpx;height:300rpx;float:left;}
.purchase-tem-text,.moduleRem-tem-text{float:right;width:360rpx;height:300rpx;position:relative;display: flex;flex-direction: column;justify-content: center;}
.purchase-tem-title{font-size:30rpx;color:#303030;width:100%;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.purchase-tem-detail{color:#999;padding:10rpx 0;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;font-size:26rpx;}
.purchase-tem-bor{display:flex;flex-direction: row;}
.purchase-tem-bor text{color:#eb5902;padding:0 6rpx;border:1px solid #eb5902;border-radius:4rpx;margin-right:5px;font-size:24rpx;}
.purchase-tem-price{padding:10rpx 0;display: flex;flex-direction: row;}
.pur-tem-price{font-size:26rpx;margin-right:10rpx;color:#303030;}
.pur-tem-price text{font-size:30rpx;color:#eb5902;}
.pur-tem{padding-left:10rpx;color:#999;padding-top:6rpx;font-size:26rpx;}
.pur-tem text{text-decoration: line-through;}
.pur-tem-btn{padding:10rpx 0;width:100%;overflow:hidden;height:50rpx;}
.stock-tem{float:left;width:200rpx;margin-top:9rpx;text-align: center;position:relative;border:1rpx solid rgb(233,90,29);border-radius: 32rpx;color:#eb5902;font-size:24rpx;height:32rpx;line-height:32rpx;}
.stock-tem text{position:absolute;left:0rpx;top:0rpx;width:0;height:100%;background:rgba(233,90,29,.2);border-radius: 16rpx;max-width:100%;}
.stock-tem.stock-other-tem{text-align: left;border:0 none;font-size:28rpx;}
.stock-tem .textradius{border-radius: 32rpx 0 0 32rpx;}
.stock-tem.stock-other-tem text{display:none;}
.state-tem{float:right;width:144rpx;height:50rpx;text-align: center;line-height:50rpx;background:#eb5902;color:#fff;border-radius: 10rpx;}
.moduleRem-tem-price{height:90rpx;padding-top:40rpx;position:relative;}
.moduleRem-tem-price text{line-height:26rpx;font-size:30rpx;color:#666;}
.moduleRem-tem-payP{color:#ed1a15 !important;font-size:34rpx !important; padding-right:70rpx; background-position: right bottom 8rpx;background-repeat: no-repeat; background-size:60rpx 25rpx;}
.moduleRem-tem-cart{width:68rpx;height:68rpx;background:#ed1a15 url(https://cache.jiangxinxiaozhen.com/WD/APP/little/saleShopcart.png)no-repeat center center;background-size:38rpx 33rpx;
position:absolute;right:20rpx;bottom:11rpx;border-radius:50%;}
.over-cart{background-color:#ddd;}
.moduleRem-tem-detail{line-height:90rpx;font-size:34rpx !important;color:#ed1a15 !important;display:inline-block; padding-top:30rpx;}
.tem-del{padding-left:20rpx; color:#777 !important;text-decoration-line:line-through;display:inline-block;line-height:90rpx;font-size:30rpx !important;}
