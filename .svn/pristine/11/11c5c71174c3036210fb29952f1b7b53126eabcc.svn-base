page{background:#f4f4f4;}
.address-wrap,.product-wrap,.balance-list-wrap{background:#f4f4f4;padding:20rpx 0 0;width:100%;overflow:hidden;}
.address-top{background:#fff url(https://cache.jiangxinxiaozhen.com/Content/wd/more1.png) no-repeat right 15rpx center;background-size:14rpx 24rpx;width:calc(100%-40rpx); padding:20rpx 40rpx 0 25rpx;overflow: hidden;}
.address-topBg{width:100%;overflow: hidden;border-radius: 18rpx;background:#fff url(https://cache.jiangxinxiaozhen.com/WD/APP/little/addressbg.png) no-repeat left bottom;background-size:100% 10rpx; padding-bottom:30rpx;}
.product-list{background:#fff;padding:20rpx 25rpx;width:100%;align-items: center;box-sizing: border-box;position:relative;}
.userMesg,.address{width:100%;overflow: hidden;}
.userMesg text,.address>text{float:left;}
.product-wrap{background:#fff;margin-top:20rpx;border-radius: 18rpx;}
.productImg{width:140rpx; height:140rpx;position:relative;margin-right:20rpx;}
.productImg>image{width:100%;height:100%;}
.productMes{padding:10rpx 0;} /*height:120rpx; */
.productImg>text{width:60rpx;height:60rpx;position:absolute;right:0;top:0;}
.balance-list-wrap{padding-bottom:100rpx;}
.balance-list{width:100%;padding:15rpx 25rpx;background:#fff;box-sizing: border-box;border-radius: 18rpx;}
.balance-list .row-center{height:88rpx;}
.balance-list>navigator{height:88rpx; line-height: 80rpx;}
.balance-list .fareDetail{padding-right:40rpx;background:url(https://cache.jiangxinxiaozhen.com/WD/public/Iicon.png)no-repeat right top 10rpx;background-size:24rpx;}
.balance-wrap{width:100%;background:#fff;line-height:90rpx;position:fixed;left:0;bottom:0;border-top:1rpx solid #f4f4f4;}
.balance-wrap>text{padding-left:25rpx;color:#eb5902;}
.balance-wrap button{background:#eb5902;color:#fff;line-height:90rpx;padding:0 20rpx;}
.payWord-wrap{width:100%;height:100%;background:rgba(0,0,0,.2);position:fixed;top:0;left:0; z-index: 10}
.payWord{position:absolute;top:50%;left:50%;width:500rpx;height:300rpx;margin-top:-150rpx;margin-left:-250rpx;background:#f8f8f8;border-radius: 10rpx;z-index: 5;overflow:hidden;}
.payTitle {font-size:30rpx;line-height:80rpx;text-align: center;width:100%;display:block;}
.inp{width:400rpx;height:60rpx;line-height:60rpx;background:#fff;display:block;margin:15rpx auto;padding-left:15rpx;box-sizing: border-box;}
.btn-bot{width:100%;border-top:1px solid #dedddd;line-height:80rpx;position:absolute;bottom:0;left:0;}
.btn-bot button{width:50%;text-align: center;color:#026bb1}
.btn-bot button:nth-child(1){border-right:1px solid #dedddd;box-sizing: border-box;color:#999;}
.productList-title{width:100%;height:80rpx;padding:0 25rpx;box-sizing: border-box;line-height:80rpx;}
.orderALL{background:#fff;padding:25rpx;}
.empty{
  background:#fff;
  height:100rpx;
  width:100%;
  position:fixed;
  top: calc(50vh - 100rpx);
  text-align: center;
  }
  .numRight-template{width:155rpx;height:50rpx;overflow:hidden;position:absolute;right:26rpx;bottom:26rpx;display:flex;flex-direction: row;justify-content: center;align-items: center;}
.numRight-template image{height:44rpx;width:48rpx;padding:5rpx;float:left;box-sizing:border-box;}
.numRight-template input{text-align: center;width:58rpx;color:#303030;font-size:30rpx;float:left;}
.storeFare{position:fixed;width:100vw;height:100vh;background:rgba(0,0,0,.2);z-index: 24;top:0;left:0;}
.fareList{width:640rpx;height:382rpx;background:#fff;position:absolute;top:50%;margin-top:-200rpx;left:50%;margin-left:-320rpx;border-radius: 20rpx;overflow: hidden;}
.fareListName{width:100%;text-align: center;line-height:80rpx;color:#303030;font-size:30rpx;}
.fareListWrap{width:calc(100%-80rpx);overflow: hidden;padding:20rpx 0 40rpx;margin:0 40rpx;border-top:1rpx solid #eee;border-bottom:1rpx solid #eee;height:160rpx;justify-content:space-around;}
.rNote{color:#999;position:absolute;bottom:100rpx;right:40rpx;font-size:22rpx;}
.fareListWrap>view{height:80rpx;width:100%;}
.iSure{position:absolute;bottom:0;left:0;width:100%;height:80rpx;}
.arrow {
  display: flex;
  justify-content: center;
  align-content: center;
}
.a_arrow {
  width:24rpx;
  height:24rpx;
  margin-left: 10rpx;
}
.a_arrow .a_right {
  display: inline-block;
  width:14rpx;
  height:20rpx;
}
.a_arrow .a_bot {
  display: inline-block;
  width:24rpx;
  height:14rpx;

}
.promotions {
  width: 658rpx;
  height: auto;
  padding: 18rpx 30rpx 0rpx 30rpx;
  background-color: #eee;
  border-radius: 6rpx;
  margin: auto;
  box-sizing: border-box;
  overflow: hidden;
}
.p_item {
  height: auto;
  font-size: 26rpx;
  color: #666;
  margin-bottom: 30rpx;
  display: flex;
  justify-content: space-between;
}
.p_fl {
  width: 74%;
}