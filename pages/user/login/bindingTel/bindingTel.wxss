.title-top{font-size:34rpx;font-weight: 600;margin:114rpx auto 80rpx;color:#303030;text-align: center;}
.login-wrap{width:630rpx;overflow: hidden;margin:0 auto;}
.tel,.checknum,.checkBtn,.loginBtn{width:100%;height:90rpx;border-radius: 45rpx !important;background:#eee;box-sizing: border-box;padding-left:50rpx;margin-bottom:44rpx;line-height:90rpx;}
.checknum{width:390rpx;}
.checkBtn{width:214rpx;padding:0;text-align: center !important;}
.loginBtn,.wxloginBtn{background:none;border:1px solid #eb5902;color:#eb5902; height:86rpx;line-height:86rpx;margin-top:74rpx;font-weight: 600;padding:0;}
.orLine{justify-content: center;margin:52rpx auto;}
.orLine text:nth-child(1),.orLine text:nth-child(3){width:65rpx;height:1rpx;background:#ddd;}
.orLine text:nth-child(2){padding:0 30rpx;color:#999;font-size:30rpx;}
.wxloginBtn{color:#fff;border:0 none; background:#23c22c;border-radius: 45rpx !important; margin-top:52rpx;}
.logoPng{width:224rpx;display:block;margin:160rpx auto 274rpx; height:224rpx;}

