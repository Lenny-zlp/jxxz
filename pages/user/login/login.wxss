.login-wrap{width:600rpx;margin:60rpx auto;overflow:hidden;}
.log{display:block;width:208rpx;height:194rpx;margin:20rpx auto 60rpx;}
.inpt{width:100%;height:100rpx;border-bottom:1px solid #eee;align-items: center;}
.inpt button{margin:0;padding:0rpx 12rpx;height:60rpx;line-height:60rpx;border:1rpx solid #eb5902 !important;border-radius: 3px !important;color:#eb5902; }
.loginBtn{width:100%;background:#eb5902;color:#fff;font-size:30rpx;line-height:88rpx;border-radius: 5px !important;text-align: center;margin:60rpx 0 15rpx;}
.wxIcon{width:68rpx;height:56rpx;opacity: 0.7;margin:40rpx auto;display:block;}
.otherLogin{padding:20rpx 0 40rpx;position:relative;width:100%;height:2rpx;border-bottom:1px solid #eee;}
.otherLogin text{position:absolute;left:0;top:29rpx;bottom:1px solid #eee;width:100%;height:1rpx;display:block;}
.otherLogin::after{position:absolute;content:'其他账号登录';width:200rpx;line-height:40rpx;background:#fff;left:50%;margin-left:-100rpx;bottom:-31rpx;margin-bottom:10rpx;text-align: center;color: #999;}
.authorWrap{width:100%;height:100%;position:fixed;top:0;left:0;background:rgba(0,0,0,.6)}
.author{width:580rpx;height:540rpx;position:absolute;top:0;left:0;right:0;bottom:0;margin:auto;background:#fff;border-radius: 20rpx;overflow:hidden;}
.btn-wrap{position:absolute;bottom:0;left:0;width:100%;}
.author image{width:100rpx;height:100rpx;margin:50rpx auto 34rpx;display:block;}
.textWrap{width:100%;text-align: center;color:#303030;font-size:30rpx;}
.author button{background:#eb5902;color:#fff;border-radius: 80rpx !important;width:480rpx;height:80rpx;position:absolute;bottom:60rpx;left:50%;margin-left:-240rpx;display:flex;align-items: center;justify-content: center;font-size:30rpx;}
.title{padding:26rpx 0;width:100%;text-align: center;font-size:34rpx;font-weight: 500;color:#303030;border-bottom:1px solid #eee;}
.author button[disabled]{background:#eb5902 !important;color:#fff !important;}
