.success-wrap {
  display: flex;
  flex-direction: column;
  justify-content: center;
  overflow: hidden;
  padding: 25rpx;
  box-sizing: border-box;
}

.success-wrap>view {
  font-size: 32rpx;
  text-align: center;
  margin-bottom: 50rpx;
  color:#eb5902;
}
.success-wrap>.text{font-size:28rpx;color: #666;}
