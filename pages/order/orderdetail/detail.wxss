/* pages/order/orderdetail/detail.wxss */

page {
  background-color: #f4f4f4;
}

.main {
  width:700rpx;
  margin:0 25rpx;
  overflow: hidden;
  height: 80%;
}

.split_view {
  position: relative;
  overflow: hidden;
  padding: 20rpx 20rpx;
 background:#fff;
 margin-bottom:20rpx;
 border-radius: 20rpx;
}
.addressBG{padding-bottom:30rpx;background:#fff url(https://cache.jiangxinxiaozhen.com/WD/APP/little/addressbg.png) no-repeat left bottom;background-size:700rpx 10rpx}
.status_str {
  line-height: 55rpx;
  color: #eb5902;
  font-size: 30rpx;
}

.group_str_active {
  padding: 2rpx 20rpx;
  width: 120rpx;
  background: #dc2929;
  color: #fff;
  line-height: 30rpx;
  border-radius: 5rpx;
}

.group_str_success {
  padding: 2rpx 20rpx;
  background: #dba860;
  color: #fff;
  line-height: 30rpx;
  border-radius: 5rpx;
}

.group_str_expire {
  padding: 2rpx 20rpx;
  width: 120rpx;
  background: #999;
  color: #fff;
  line-height: 30rpx;
  border-radius: 5rpx;
}

.logisticsmsg {
  float: left;
  font-size: 28rpx;
  padding: 8rpx 40rpx 8rpx 0;
  color: #303030;
  overflow: hidden;
 
  line-height: 40rpx;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  width: 100%;
  box-sizing: border-box;
}

.logisticstime {
  clear: both;
  font-size: 28rpx;
  padding: 8rpx 0;
  color: #888;
}

.group_head{
  border: 1px solid #eb5902;
  color: #eb5902;
  padding:0 4rpx;
  font-size: 24rpx;
  border-radius:5rpx;
  margin-left:10rpx;
}

.reciver {
  float: left;
  font-size: 28rpx;
  padding: 8rpx 0;
  color: #303030;
}

.mobile {
  float: right;
  font-size: 28rpx;
  padding: 8rpx 33rpx 0 0;
  color: #303030;
}

.address {
  clear: both;
  font-size: 28rpx;
  padding: 8rpx 33rpx 0 0;
  color: #303030;
}

.split_view_v1 {
  clear: both;
  overflow: hidden;
  padding: 10rpx 0;
}

.left_str {
  float: left;
  color: #303030;
  font-size: 28rpx;
}

.right_str {
  float: right;
  color: #303030;
  font-size: 28rpx;
}

.ordercode_l {
  float: left;
  color: #303030;
  font-size: 28rpx;
}

.ordercode_r {
  float: right;
  color: #303030;
}

.copy_btn {
  padding: 5rpx 15rpx;
  width: 85rpx;
  font-size: 25rpx;
  line-height: 30rpx;
  border-radius: 3px;
  border: solid 1rpx #888;
}

.product_img_view {
  float: left;
  position: relative;
 
  background: #fff;
}

.order_product {
  position: relative;
  width: 100%;
  overflow: hidden;
  padding:20rpx 0;
 
}

/* 仓号一行 */
.order_store {
  height: 80rpx;
  line-height: 80rpx;
  padding-left: 2%;
  font-size: 26rpx;
  color: #666;
}

.product_img {
  width: 140rpx;
  height: 140rpx;
  display: block;
  border: 0 none;
  padding: 2px;
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}

.image_icon {
  width: 53rpx;
  height: 53rpx;
  position: absolute;
  right: 0;
  top: 0;
}

.image_icon image {
  width: 53rpx;
  height: 53rpx;
}

.product_text {
  width: 60%;
  padding: 5rpx 20rpx;
  float: left;
}

.product_text view {
  line-height: 48rpx;
  font-size: 26rpx;
}

.product_text view:nth-child(2) {
  line-height: 40rpx;
  font-size: 22rpx;
  color: #666;
}

.product_qty {
  position: absolute;
  right: 30rpx;
  top: 40%;
  color: #666;
  font-size: 26rpx;
}

.bottom_btn {
  position: fixed;
  bottom: 0;
  right: 0;
  width: 100%;
  padding: 15rpx 0;
  background: #fff;
  margin-top: 40rpx;
}

.del_button {
  width: 150rpx;
  height: 50rpx;
  font-size: 28rpx;
  padding: 10rpx 10rpx;
  line-height: 30rpx;
  background-color: #fff;
  color: #000;
  border: solid 1rpx #000;
  float: right;
  margin-right: 20rpx;
}

.pay_button {
  width: 150rpx;
  height: 50rpx;
  font-size: 28rpx;
  padding: 10rpx 10rpx;
  line-height: 30rpx;
  color: #eb5902;
  border: solid 1px #eb5902;
  background-color: #fff;
  float: right;
  margin-right: 20rpx;
}

.p_content {
  height: 380rpx;
  font-size: 30rpx;
  color: black;
}

.p_success {
  margin-bottom: 36rpx;
  font-weight: bold;
  font-size: 32rpx;
  color: black;
}

.p_time {
  width: 40rpx;
  height: 40rpx;
  border: solid 1rpx #666;
  border-radius: 40px;
  padding: 1rpx;
  float: left;
  line-height: 35rpx;
}

.p_per {
  margin-top: 120rpx;
  position: absolute;
  left: calc(3%);
  width: 100%;
}

.p_line_l {
  border: solid 1rpx #888;
  width: 60rpx;
  float: left;
  margin-left: 1px;
}

.p_line_r {
  border: solid 1rpx #888;
  width: 60rpx;
  float: left;
}

.p_line_c {
  float: left;
  line-height: 5rpx;
  color: #888;
  margin-left: 6rpx;
  margin-right: 6rpx;
}

.p_btn {
  border: solid 1rpx #eb5902;
  width: 80%;
  margin: 0 auto;
  height: 72rpx;
  line-height: 70rpx;
  background: #eb5902;
  color: #eee;
  border-radius: 40rpx !important;
  font-size: 28rpx;
}

.popup-title {
  text-align: center;
  font-size: 32rpx;
  font-weight: 600;
  color: #303030;
  margin: 0 auto;
}

.popup-title text {
  color: #eb5902;
  font-size: 32rpx;
  font-weight: 600;
}

.popup-time {
  font-size: 28rpx;
  color: #303030;
  margin-top: 60rpx;
  justify-content: center;
}

.popup-time text {
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  border: 1px solid #dedddd;
  text-align: center;
  line-height: 38rpx;
  font-size: 20rpx;
  border-radius: 20rpx;
  margin-right: 10rpx;
  margin-left: 10rpx;
  box-sizing: border-box;
}

.popup-pensonal {
  position: relative;
  width: 100%;
  text-align: center;
  margin: 40rpx auto 20rpx;
  height: 44rpx;
}

.popup-pensonal>text {
  position: absolute;
  width: 70%;
  background: #fff;
  text-align: center;
  left: 15%;
  margin-top:10rpx;
}

.popup-pensonal::before {
  position: absolute;
  width: 90%;
  height: 2rpx;
  background: #999;
  left: 5%;
  top: 50%;
  margin-top: 1rpx;
  content: "";
}

.popuo-btn {
  width: 65%;
  height: 78rpx;
  border-radius: 44rpx !important;
  color: #fff;
  text-align: center;
  line-height: 74rpx;
  background: #eb5902;
  margin: 62rpx auto 50rpx;
}
.salebox {
  display: flex;
  flex-direction: row-reverse;
  padding-right: 34rpx;
  padding-bottom: 18rpx;
}
.afterSale {
  border: 2rpx solid #CCC;
  width: 140rpx;
  height: 46rpx;
  font-size: 26rpx;
  color: #303030;
  line-height: 46rpx;
  text-align: center;
  border-radius: 23rpx;
}
.dialog {
  position: fixed;
  top: 0;
  left: 0;
  width: 750rpx;
  height: 100vh;
  background-color: rgba(0,0,0,.5);
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  z-index: 6;
}
.dialog .font {
  padding: 50rpx;
  width: 70%;
  min-height: 80rpx;
  line-height: 40rpx;
  border-bottom: 2rpx solid #CCC;
  background-color: #fff;
  text-align: center;
  border-radius: 15rpx 15rpx 0 0;
}
.dialog .btn {
  width: 70%;
  padding: 0 50rpx;
  height: 80rpx;
  line-height: 80rpx;
  color: rgb(110, 146, 245);
  background-color: #fff;
  text-align: center;
  border-radius: 0 0 15rpx 15rpx;
}
.arrow {
  display: flex;
  justify-content: center;
  align-content: center;
}
.a_arrow {
  width:24rpx;
  height:24rpx;
  margin-left: 6rpx;
}
.a_arrow .a_right {
  display: inline-block;
  width:24rpx;
  height:14rpx;
  transform: rotate(-90deg);
  position: relative;
  top: -4rpx;
}
.a_arrow .a_bot {
  display: inline-block;
  width:24rpx;
  height:14rpx;

}
.promotions {
  width: 658rpx;
  height: auto;
  padding: 18rpx 30rpx 0rpx 30rpx;
  background-color: #eee;
  border-radius: 6rpx;
  margin: auto;
  box-sizing: border-box;
  overflow: hidden;
}
.p_item {
  height: auto;
  font-size: 26rpx;
  color: #666;
  margin-bottom: 20rpx;
  display: flex;
  justify-content: space-between;
}
.p_fl {
  width: 74%;
}